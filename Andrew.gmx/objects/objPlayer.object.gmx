//check air
if (!place_meeting(x, y+1, collision_object)) {
    if (vspd &gt; 0) { // if falling
        image_index = 8;
    } else { // else going up
        image_index = 7;
    }
} else {
    //on ground
    //check moving
    if (hspd !=0) {
        image_speed = 0.5;
        if 1 &gt; image_index or image_index &gt; 7 {
            image_index = 1;
        }
    } else {
        //idle
        image_index  = 0;
    }
}

